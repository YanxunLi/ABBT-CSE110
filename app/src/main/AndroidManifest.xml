<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.afs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launch_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launch_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login" />
        <activity android:name=".WorkOut" />
        <activity android:name=".DietPlan" />
        <activity android:name=".Calendar" />
        <activity android:name=".Profile" />
        <activity android:name=".ExerciseList" />
        <activity android:name=".Signup" />
        <activity android:name=".AddFood" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FoodHistory" />
        <activity android:name=".AddFood" />
    </application>

</manifest>