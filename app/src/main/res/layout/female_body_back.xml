<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="420dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/female_body_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/female_body_back" />

        <Button
            android:id="@+id/female_back_ticep_left"
            android:layout_width="22dp"
            android:layout_height="80dp"
            android:layout_below="@+id/female_back_shoulder_left"
            android:layout_toLeftOf="@+id/female_back_back"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_ticep_right"
            android:layout_width="22dp"
            android:layout_height="80dp"
            android:layout_below="@+id/female_back_shoulder_right"
            android:layout_marginRight="1dp"
            android:layout_toRightOf="@+id/female_back_back"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_butt"
            android:layout_width="80dp"
            android:layout_height="35dp"
            android:layout_above="@+id/female_back_legs"
            android:layout_centerHorizontal="true"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_back"
            android:layout_width="65dp"
            android:layout_height="95dp"
            android:layout_above="@+id/female_back_butt"
            android:layout_centerHorizontal="true"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_legs"
            android:layout_width="65dp"
            android:layout_height="180dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="20dp"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_shoulder_left"
            android:layout_width="45dp"
            android:layout_height="20dp"
            android:layout_above="@+id/female_back_back"
            android:layout_marginLeft="10dp"
            android:background="@android:color/transparent" />

        <Button
            android:id="@+id/female_back_shoulder_right"
            android:layout_width="45dp"
            android:layout_height="20dp"
            android:layout_above="@+id/female_back_back"
            android:layout_alignRight="@+id/female_body_back"
            android:layout_marginRight="10dp"
            android:background="@android:color/transparent" />
    </RelativeLayout>

</RelativeLayout>
